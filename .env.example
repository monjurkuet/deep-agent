# Environment Variables

# Ollama Configuration
OLLAMA_BASE_URL=http://localhost:11434

# Model names
OLLAMA_PLANNING_MODEL_NAME=ministral-3:latest
OLLAMA_EXECUTION_MODEL_NAME=ministral-3:latest

# Temperatures
OLLAMA_PLANNING_TEMPERATURE=0.7
OLLAMA_EXECUTION_TEMPERATURE=0.0

# Backend Configuration
# BACKEND_TYPE=state  # Ephemeral storage (default)
# BACKEND_TYPE=composite  # Hybrid: ephemeral + persistent (requires BACKEND_ROUTES)

# For composite backend, specify routes (optional)
# Example: BACKEND_ROUTES={"/memories/": "lambda rt: StoreBackend(rt)"}

# Logging
LOG_LEVEL=INFO
